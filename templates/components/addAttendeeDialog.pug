md-dialog(layout-padding)
	form(name='addAttendeeForm')
		div(layout='row')
			md-input-container
				label First Name
				input(required, ng-model='attendee.first')
			md-input-container
				label Last Name
				input(required, ng-model='attendee.last')
		md-input-container
			label Access
			input(ng-model='attendee.access')
		div(layout='row')
			md-button(flex,ng-click='cancel()') Cancel
			md-button.md-raised.md-primary(flex, ng-click='addAttendee(attendee)') Add Attendee
